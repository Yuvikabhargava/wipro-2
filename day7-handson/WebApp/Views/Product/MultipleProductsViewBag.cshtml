@{
    ViewData["Title"] = "Multible Product using View Bag";
}
<h1>Product List</h1>
@foreach (var product in ViewBag.Products){
    <ul>
        <li>Product Code : @product.PCode</li>
        <li>Name : @product.Name</li>
        <li>Quantity in Stock : @product.QtyInStock</li>
        <li>Details : @product.Details</li>
        <li>Price : @product.Price</li>
    </ul>
}
